function [data_m] = window_smooth(data,n)
%n为窗口长度
L = length(data);%数据长度
 
%均值滤波
m = 0;
k = 0;
for i =1:L
    if(i<n)
        data_m(i) = data(i);
    else
       for(j = i-n+1:i)
           k = k+1;
       w1(k) = data(j);
       end
    data_m(i) = mean(w1);
    k = 0;
    end
end
